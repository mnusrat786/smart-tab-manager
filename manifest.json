{
  "manifest_version": 3,
  "name": "Smart Tab Manager",
  "version": "1.0",
  "description": "AI-powered tab management to organize and optimize your browsing",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Smart Tab Manager"
  }
}